<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <title>Emilia's Cafe</title>
  <link rel="stylesheet" href="/css/register.css">
</head>

<body>
  <script src="/js/jquery-3.2.1.min.js"></script>
  <script src="/js/register.js"></script>

  <div class="container">
    <div class="formTitle">
      Sign Up
    </div>
    <form method="post">
      <div class="inputCon">
        <span class="input">
          <input class="input__field" type="text" id="input-1" name="username" spellcheck="false" />
          <label class="input__label" for="input-1">
            <span class="input__label-content">Username:</span>
          </label>
        </span>

        <span class="input">
            <input class="input__field" type="text" id="input-2" name="stuid" spellcheck="false" />
            <label class="input__label" for="input-2">
              <span class="input__label-content">StuID:</span>
            </label>
          </span>

          <div class="msg">
              <span class="msgWords">123</span>
            </div>
          <div class="msg msgRight">
            <span class="msgWords">123</span>
          </div>
      </div>

      <div class="inputCon">
        <span class="input">
          <input class="input__field" type="text" id="input-3" name="tel" spellcheck="false" />
          <label class="input__label" for="input-3">
            <span class="input__label-content">Tel:</span>
          </label>
        </span>

        <span class="input">
            <input class="input__field" type="text" id="input-4" name="email" spellcheck="false" />
            <label class="input__label" for="input-4">
              <span class="input__label-content">Email:</span>
            </label>
          </span>

          <div class="msg">
              <span class="msgWords">123</span>
            </div>
          <div class="msg msgRight">
            <span class="msgWords">123</span>
          </div>
      </div>

      <div class="inputCon">
          <span class="input">
            <input class="input__field" type="password" id="input-5" name="password" spellcheck="false" />
            <label class="input__label" for="input-5">
              <span class="input__label-content">Pass:</span>
            </label>
          </span>
  
          <span class="input">
              <input class="input__field" type="password" id="input-6" name="repassword" spellcheck="false" />
              <label class="input__label" for="input-6">
                <span class="input__label-content">ReapeatPass:</span>
              </label>
            </span>
  
            <div class="msg">
                <span class="msgWords">123</span>
              </div>
            <div class="msg msgRight">
              <span class="msgWords">123</span>
            </div>
        </div>

      

      <div class="buttons">
        <input type="reset" value="Reset" class="radiusButton reset">
        <button class="radiusButton submit">Submit</button>
      </div>
    </form>

    <div class="success hidden">Sign Up Succeeded!</div>
  </div>

</body>

</html>